{
  "name": "es-cjs",
  "version": "1.0.0",
  "description": "",
  "main": "./cjs/index.js",
  "module": "./es2015/index.js",
  "types":"./es2015/index.d.ts",
  "scripts": {
    "build": "run-p --aggregate-output -c --print-label build:cjs build:es2015 build:es2020 build:es2022 build:node16 build:nodenext",
    "build:cjs": "tsc -p tsconfig.cjs.json || ncp ../package.cjs.json cjs/package.json",
    "build:es2015": "tsc -p tsconfig.es2015.json || ncp ../package.cjs.json cjs/package.json",
    "build:es2020": "tsc -p tsconfig.es2020.json || ncp ../package.cjs.json cjs/package.json",
    "build:es2022": "tsc -p tsconfig.es2022.json || ncp ../package.cjs.json cjs/package.json",
    "build:node16": "tsc -p tsconfig.node16.json",
    "build:nodenext": "tsc -p tsconfig.nodenext.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "5.0.0-dev.20230103"
  }
}

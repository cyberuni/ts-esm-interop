{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "build:cjs": {
      "dependsOn": [
        "^build"
      ]
    },
    "build:es2015": {
      "dependsOn": [
        "^build"
      ]
    },
    "build:es2020": {
      "dependsOn": [
        "^build"
      ]
    },
    "build:es2022": {
      "dependsOn": [
        "^build"
      ]
    },
    "build:node16": {
      "dependsOn": [
        "^build"
      ]
    },
    "build:nodenext": {
      "dependsOn": [
        "^build"
      ]
    },
    "clean": {
      "cache": false
    },
    "test": {},
    "test:cjs": {},
    "test:es2015": {},
    "test:es2020": {},
    "test:es2022": {},
    "test:node16": {},
    "test:nodenext": {}
  }
}

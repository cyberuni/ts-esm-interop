{
  "name": "nodenext",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "exports": {
    "types": "./nodenext/index.d.ts",
    "import": "./nodenext/index.js",
    "require": "./commonjs/index.js"
  },
  "main": "./commonjs/index.js",
  "types": "./commonjs/index.d.ts",
  "scripts": {
    "build": "run-p --aggregate-output -c --print-label build:commonjs build:es2015 build:es2020 build:es2022 build:esnext build:node16 build:nodenext",
    "build:commonjs": "run-s -c build:commonjs:tsc build:commonjs:ncp",
    "build:commonjs:tsc": "tsc -p tsconfig.commonjs.json",
    "build:commonjs:ncp": "ncp ../../package.commonjs.json commonjs/package.json",
    "build:es2015": "run-s -c build:es2015:tsc build:es2015:ncp",
    "build:es2015:tsc": "tsc -p tsconfig.es2015.json",
    "build:es2015:ncp": "ncp ../../package.commonjs.json es2015/package.json",
    "build:es2020": "run-s -c build:es2020:tsc build:es2020:ncp",
    "build:es2020:tsc": "tsc -p tsconfig.es2020.json",
    "build:es2020:ncp": "ncp ../../package.commonjs.json es2020/package.json",
    "build:es2022": "run-s -c build:es2022:tsc build:es2022:ncp",
    "build:es2022:tsc": "tsc -p tsconfig.es2022.json",
    "build:es2022:ncp": "ncp ../../package.commonjs.json es2022/package.json",
    "build:esnext": "run-s -c build:esnext:tsc build:esnext:ncp",
    "build:esnext:tsc": "tsc -p tsconfig.esnext.json",
    "build:esnext:ncp": "ncp ../../package.commonjs.json esnext/package.json",
    "build:node16": "tsc -p tsconfig.node16.json",
    "build:nodenext": "tsc -p tsconfig.nodenext.json",
    "clean": "rimraf commonjs es* node16 nodenext",
    "test": "ava",
    "test:commonjs": "ava commonjs/*.all.js commonjs/*.cjs.js",
    "test:es2015": "ava es2015/*.all.js es2015/*.es.js es2015/*.es2015.js",
    "test:es2020": "ava es2020/*.all.js es2020/*.es.js es2020/*.es2020.js",
    "test:es2022": "ava es2022/*.all.js es2022/*.es.js es2022/*.es2022.js",
    "test:esnext": "ava esnext/*.all.js esnext/*.es.js esnext/*.esnext.js",
    "test:node16": "ava node16/*.all.js node16/*.node.js node16/*.node16.js",
    "test:nodenext": "ava nodenext/*.all.js nodenext/*.node.js nodenext/*.nodenext.js",
    "test:assert": "ava */assert-*.js",
    "test:assertron": "ava */assertron-*.js",
    "test:cjs": "ava */cjs-*.js",
    "test:es-cjs": "ava */es-cjs-*.js",
    "test:esm": "ava */esm-*.js",
    "test:esm-cjs": "ava */esm-cjs-*.js",
    "test:param-case": "ava */param-case-*.js"
  },
  "ava": {
    "files": [
      "commonjs/*.js",
      "es2015/*.js",
      "es2020/*.js",
      "es2022/*.js",
      "node16/*.js",
      "nodenext/*.js"
    ]
  },
  "dependencies": {
    "assertron": "^7",
    "cjs": "workspace:*",
    "color-map": "^1",
    "es-cjs": "workspace:*",
    "esm": "workspace:*",
    "esm-cjs": "workspace:*",
    "param-case": "1"
  },
  "devDependencies": {
    "@types/node": "^14",
    "@types/react": "^17",
    "ava": "^5.1.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "5.0.0-dev.20230103"
  }
}
